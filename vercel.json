{
  "version": 2,
  "builds": [{ "src": "index.js", "use": "@vercel/node" }],
  "routes": [{ "src": "/api/(.*)", "dest": "/index.js" }],
  "headers": [
    {
      "source": "/api/*",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://profolio-front-2v.vercel.app"
        }
      ]
    }
  ]
}
